{
  "name": "cerebral-pouchdb",
  "version": "0.0.1",
  "description": "A cerebral module for integrating the cerebral store with one or more pouch dbs",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "dev": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"eslint src test && mocha --compilers js:babel/register --recursive --reporter dot\" src test'",
    "prebuild": "npm run lint",
    "build": "babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/cerebral-pouchdb.git"
  },
  "keywords": [
    "cerebral",
    "pouchdb",
    "couchdb"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/cerebral-pouchdb/issues"
  },
  "homepage": "https://github.com/garth/cerebral-pouchdb#readme",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "pouchdb": "^5.1.0",
    "watch": "^0.17.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "cerebral-modules": "^0.3.0",
    "pouchdb": "^5.1.0"
  }
}
